# Terraform state files
*.tfstate
*.tfstate.*
*.tfstate.backup

# Crash log files
crash.log
crash.*.log

# Exclude all .tfvars files, which are likely to contain sensitive data
*.tfvars
*.tfvars.json

# Ignore override files as they are usually used to override resources locally
override.tf
override.tf.json
*_override.tf
*_override.tf.json

# Ignore CLI configuration files
.terraformrc
terraform.rc

# Terraform dependency lock file (keep this if you want consistent provider versions)
# Uncomment the line below if you want to ignore it
# .terraform.lock.hcl

# Terraform plugins and modules downloaded by terraform init
.terraform/
.terraform.lock.hcl

# Terraform plan output files
*.tfplan
*.tfplan.*
plan.out
tfplan

# Ignore any generated files from terraform apply
.terraform.tfstate.lock.info

# Ignore backup files
*~
*.bak

# Ignore Mac OS files
.DS_Store

# Ignore any local environment-specific files
local.tfvars
secrets.tfvars
*.auto.tfvars
